{
  "name": "archon/dataframe",
  "description": "Archon: PHP Data Analysis Library",
  "keywords": ["data", "data analysis", "sql", "csv", "database", "excel"],
  "homepage": "https://github.com/HWGehring/Archon",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.2",
    "ext-mbstring": "*",
    "ext-pdo_sqlite": "*",
    "gajus/dindent": "dev-master",
    "phpoffice/phpspreadsheet": "^1.29"
  },
  "require-dev": {
    "laravel/pint": "^1.13",
    "pestphp/pest": "^2.20"
  },
  "autoload": {
    "psr-4": { "Archon\\": "src/"}
  },
  "autoload-dev" : {
    "psr-4": { "Archon\\Tests\\": "tests/" }
  },
  "scripts": {
    "test": "pest",
    "pintify": "pint"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
