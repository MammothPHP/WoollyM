{
  "name": "archon/dataframe",
  "description": "Archon: PHP Data Analysis Library",
  "keywords": ["data", "data analysis", "sql", "csv", "database", "excel"],
  "homepage": "https://github.com/HWGehring/Archon",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.2",
    "ext-mbstring": "*",
    "ext-pdo_sqlite": "*",
    "phpoffice/phpexcel": "1.8.1",
    "gajus/dindent": "2.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3"
  },
  "autoload": {
    "psr-4": { "Archon\\": "src/"}
  },
  "autoload-dev" : {
    "psr-4": { "Archon\\Tests\\": "tests/" }
  },
  "scripts": {
    "test": "phpunit --display-deprecations",
    "pintify": "pint"
  }
}
